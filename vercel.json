{
  "version": 2,
  "functions": {
    "api/chat.py": {
      "runtime": "python3.9"
    },
    "api/index.py": {
      "runtime": "python3.9"
    }
  },
  "routes": [
    {
      "src": "/api/chat",
      "dest": "/api/chat.py"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ]
} 